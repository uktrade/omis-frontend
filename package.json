    "dotenv": "^4.0.0",
    "express": "4.15.4",
    "morgan": "1.8.2",
    "nunjucks": "3.0.1",
    "raven": "2.1.2",
    "request": "2.81.0",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "compression": "1.7.0",
    "eslint": "4.5.0",
