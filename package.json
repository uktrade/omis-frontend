{
  "name": "omis-frontend",
  "version": "1.0.0",
  "description": "The public front end for OMIS",
  "main": "src/app/app.js",
  "engines": {
    "node": "16.18.1"
  },
  "scripts": {
    "start": "NODE_ENV=production node --use-strict src/app/server.js",
    "develop": "run-p watch:app:*",
    "clean": "del .build",
    "build": "npm run clean && webpack",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=development; LOG_LEVEL=error; JASMINE_CONFIG_PATH=src/test/app/jasmine.json jasmine",
    "lint": "npm-run-all lint:all:*",
    "lint:all:js": "eslint ./test ./src",
    "lint:modified:js": "CHANGED_FILES=$(git diff --staged --name-only -- '*.js'); if [ \"$CHANGED_FILES\" ]; then eslint $(echo $CHANGED_FILES); fi",
    "watch:app:client": "webpack --watch --progress",
    "watch:app:server": "nodemon $NODE_DEBUG_OPTION --use-strict",
    "heroku-postbuild": "npm rebuild && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uktrade/omis-frontend.git"
  },
  "contributors": [
    "Dom Smith <dom@teneightfive.com> (http://github.com/teneightfive)",
    "Graham Veal (https://github.com/web-bert)",
    "Zac Tolley <zac@thetolleys.com> (https://github.com/ztolley)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uktrade/omis-frontend/issues"
  },
  "homepage": "https://github.com/uktrade/omis-frontend#readme",
  "dependencies": {
    "axios": "^0.21.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "case": "^1.5.5",
    "client-sessions": "^0.8.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.11",
    "csurf": "^1.9.0",
    "date-fns": "^1.29.0",
    "del-cli": "^1.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-sslify": "^1.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-sass-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "govuk_frontend_toolkit": "^7.5.0",
    "hawk": "^9.0.1",
    "image-webpack-loader": "^4.3.0",
    "lodash": "^4.17.10",
    "markdown-it": "^8.4.1",
    "memorystore": "^1.6.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "nunjucks": "^3.1.3",
    "nunjucks-markdown": "^2.0.1",
    "postcss-loader": "^2.1.5",
    "raven": "^2.6.2",
    "request": "^2.87.0",
    "resolve-url-loader": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "style-loader": "^0.21.0",
    "webpack": "^3.12.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-merge": "^4.1.2",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "nock": "^12.0.3",
    "nodemon": "^1.17.5",
    "proxyquire": "^2.0.1"
  },
  "overrides": {
    "is-svg": "4.3.0",
    "nth-check": "2.0.1",
    "object-path": "0.11.8",
    "semver-regex": "3.1.3",
    "trim-newlines": "3.0.1",
    "glob-parent": "5.1.2",
    "js-yaml": "3.13.1",
    "got": "11.8.5"
  }
}
